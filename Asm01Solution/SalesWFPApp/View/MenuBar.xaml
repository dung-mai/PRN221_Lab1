<UserControl x:Class="SalesWFPApp.View.MenuBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SalesWFPApp.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel Name="topMenuAdmin" Height="22" VerticalAlignment="Top">
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Product" Click="MenuItem_Click" >
            </MenuItem>
            <MenuItem Header="Member" Click="MenuItem_Click_1">
            </MenuItem>
            <MenuItem Header="Order" Click="MenuItem_Click_2">
            </MenuItem>
            <MenuItem Header="Order Detail" Click="MenuItem_Click_3">
            </MenuItem>
            <MenuItem Header="Log out" Click="MenuItem_Click_4">
            </MenuItem>
        </Menu>
    </DockPanel>
</UserControl>
